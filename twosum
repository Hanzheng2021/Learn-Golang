func twoSum(nums []int, target int) []int {
    var result = []int{0,0}
    //var  v1,v2 int
    for i,v1 := range nums {
        for j,v2 := range nums{
            if (v1+v2 == target) && (i!=j) {
                result[0]=i
                result[1]=j

            }
        }
    }
                return result
}
